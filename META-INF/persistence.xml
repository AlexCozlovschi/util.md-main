<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">
    <class>com.util.md.data.entity.Addresses</class>
    <class>com.util.md.data.entity.ArchivedTasks</class>
    <class>com.util.md.data.entity.Categories</class>
    <class>com.util.md.data.entity.Services</class>
    <class>com.util.md.data.entity.Tasks</class>
    <class>com.util.md.data.entity.Users</class>
    <class>com.util.md.data.entity.Data</class>
    <properties>
      <property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/util"/>
      <property name="hibernate.connection.driver_class" value="com.mysql.cj.jdbc.Driver"/>
    </properties>
  </persistence-unit>
</persistence>
